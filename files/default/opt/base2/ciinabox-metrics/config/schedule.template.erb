<%  frequencies.each do |frequency| %>

  <%=frequency%> do
    # Install <%= frequency %> cron
    rake "ciinabox:process_metric_cron[\"<%=frequency%>\"]"
  end

<% end %>

every 1.hour do
  # Update rake tasks
  rake "ciinabox:install_metrics_cron"
end

every 10.minutes do
  # Update rake tasks
  rake "ciinabox:write_instance_info"
end